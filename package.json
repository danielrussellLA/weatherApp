  "main": "index.js",
  "scripts": {
    "dev:build-frontend": "webpack --config webpack.frontend.js --mode=development -w",
    "dev:build-server": "webpack --config webpack.server.js --mode=development -w",
    "dev:start-server": "nodemon ./server-build/index.js",
    "prod:build-frontend": "webpack --config webpack.frontend.js --mode=production",
    "prod:build-server": "webpack --config webpack.server.js --mode=production",
    "prod:start-server": "node ./server-build/index.js",
    "clean": "rm -rf ./build",
    "dev": "NODE_ENV=development npm-run-all clean --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:build*",
    "start": "NODE_ENV=production npm-run-all prod:start*",
    "prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "pre-commit": "lint-staged",
    "lint": "eslint --max-warnings=0 .",
    "test": "jest"
  },
